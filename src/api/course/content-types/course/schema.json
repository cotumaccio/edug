{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "blocks"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "beginner",
        "intermediate",
        "advanced"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "published": {
      "type": "boolean"
    },
    "lessons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lesson.lesson",
      "mappedBy": "courses"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "course"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "courses"
    }
  }
}
